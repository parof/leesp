use std::str::FromStr;

grammar;

// type inference!
pub Term = {
    Num,
    "(" <Term> ")",
    "22" => format!("Twenty-two!"),
    ID => format!("Id({})", <>), // <-- we added this
};

match {
    r"(?i)begin" => "BEGIN",
    r"[0-9]+",
    "22"
} else {
    r"\w+" => ID, // <-- give a name here
    _
}


Num: String = r"[0-9]+" => <>.to_string();